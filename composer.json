{
    "name": "digitalrisks/lese",
    "description": "Laravel Event Sourcing Eventstore bridge layer",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Craig Morris",
            "email": "craig.morris@digitalrisks.co.uk"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "DigitalRisks\\Lese\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "DigitalRisks\\Lese\\Tests\\": "tests"
        }
    },
    "require": {
        "spatie/laravel-event-sourcing": "^3.1",
        "prooph/event-store-http-client": "^1.0@beta",
        "php-http/guzzle6-adapter": "^2.0",
        "prooph/event-store-client": "^1.0@RC"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.1",
        "orchestra/testbench": "^5.2",
        "morrislaptop/var-dumper-with-context": "^0.3.2"
    },
    "extra": {
        "laravel": {
            "providers": [
                "DigitalRisks\\Lese\\ServiceProvider"
            ]
        }
    }
}
